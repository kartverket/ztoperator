{
  "interactiveLogin": false,
  "tokenCallbacks": [
    {
      "issuerId": "idporten",
      "requestMappings": [
        {
          "requestParam": "code",
          "match": "idporten_code",
          "claims": {
            "sub": "idporten_client",
            "role": "idporten_role",
            "aud": [
              "idporten_server"
            ]
          }
        }
      ]
    },
    {
      "issuerId": "maskinporten",
      "requestMappings": [
        {
          "requestParam": "code",
          "match": "maskinporten_code",
          "claims": {
            "sub": "maskinporten_client",
            "role": "maskinporten_role",
            "aud": [
              "maskinporten_server"
            ]
          }
        }
      ]
    },
    {
      "issuerId": "entraid",
      "requestMappings": [
        {
          "requestParam": "code",
          "match": "entraid_client",
          "claims": {
            "sub": "entraid_client",
            "role": "entraid_role",
            "aud": [
              "entraid_server"
            ]
          }
        },
        {
          "requestParam": "code",
          "match": "entraid_user",
          "claims": {
            "sub": "entraid_user",
            "role": ["entraid_role", "entra_user_role"],
